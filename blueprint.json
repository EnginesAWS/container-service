{
	"identifier": "container-service",
	"about": {
		"title": "AWS Container Service",
		"explanation": "An application blueprint for defining an AWS container service"
	},
	"compute_service": {
		"default": {
			"identifier": "container_service"
		}
	},
	"binding_target": {
		"configuration": {
			"cluster": "aws_ecs_cluster.container_service_cluster.id",
			"desired_count": 3,
			"iam_role": "aws_iam_role.iam.arn",
			"depends_on": "[aws_iam_role_policy.iam]",
			"cpu": 10,
			"memory": 256,
			"essential": true,
			"container_port": 443,
			"host_port": 443
 		}
	}
}
